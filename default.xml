<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="github"
      fetch="http://github.com/"
      alias="origin"
      />
  <default review="master"
      remote="github"
      revision="master"
      sync-j="1"
      sync-c="true"
      sync-s="true"
      />
  <project name="haobug/bashrc" path=".bashrc.d" />
  <project name="haobug/git_conf" path=".gitconfig.d" />
  <project name="haobug/vimrc" path="notdofiles/vimrc">
      <linkfile src="_vim" dest=".vim" />
      <linkfile src="_vimrc" dest=".vimrc" />
  </project>
  <project name="haobug/bin" path="notdofiles/binbin">
      <linkfile src="." dest="bin" />
  </project>
</manifest>
